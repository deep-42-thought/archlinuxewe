install=$pkgname.install
source=("https://arch.eckner.net/archlinuxewe-keyring-${pkgver}.tar.gz"{,.sig})
sha512sums=('974e9ce7f4563871dc09d398a0a8fe3c3fc9c7890646e1eca3628c272eeecfdd1b05843592b9c170d3269b5e8ff28cbf9a17dba03c9d7e40e854c9bcad0daa5c'
            'SKIP')
validpgpkeys=(DE9F7688CACF04FEB81A6C590AEEC90755DA7B5A)

package() {
  cd $pkgname-$pkgver
  make PREFIX=/usr DESTDIR="$pkgdir" install
}
