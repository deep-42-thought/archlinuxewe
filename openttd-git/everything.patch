+	virtual void OnDropdownSelect(int widget, int index)
+	{
+		assert(widget == WID_CT_CONVERT_RAILTYPE);
+		if (index == INVALID_RAILTYPE) {
+			_current_clipboard->mode &= ~CPM_CONVERT_RAILTYPE;
+		} else {
+			_current_clipboard->mode |= CPM_CONVERT_RAILTYPE;
+			_current_clipboard->railtype = (RailType)index;
+		}
+		this->UpdateButtons();
+	}
