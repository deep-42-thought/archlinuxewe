+static int GetCurRes()
+{
+	int i;
+
+	for (i = 0; i != _num_resolutions; i++) {
+		if ((int)_resolutions[i].width == _screen.width &&
+				(int)_resolutions[i].height == _screen.height) {
+			break;
+		}
+	}
+	return i;
+}
