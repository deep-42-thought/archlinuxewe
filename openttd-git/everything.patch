+				break;
+
+			default:
+				NOT_REACHED();
+		}
+
+		Axis axis = DiagDirToAxis(dir);
+		while (mid_len-- > 0) {
+			tile = TileAddByDiagDir(tile, dir);
+			SetBridgeMiddle(tile, axis);
+		}
+	}
