+					*object_rect = GenericTileArea(tile, other_tile);
+				} else {
+					*object_rect = GenericTileArea(GenericTileIndex(INVALID_TILE_INDEX, MapOf(tile)), 0, 0);
+				}
+			}
+			break;
+		}
+
+		default:
