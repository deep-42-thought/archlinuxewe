    "${_srcpkgname}-${pkgver}.tar.gz::https://opensources.eckner.net/dl.php?dl=${_srcpkgname}-${pkgver}"
)
sha512sums=('523d8a38f3221890e817e79d0d494fffc266b4d1a8f434a75d46d07cfef2c708ef3319a2f6efbbb7712f4579fb32cfbdee547ad4d8d27705945584a4e1aefe28')

package() {

    ls -la

    cd ${_srcpkgname}-${pkgver}
