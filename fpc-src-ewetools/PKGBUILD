license=('GPL')
groups=()
depends=()
makedepends=()
checkdepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
options=()
source=(
    "${_srcpkgname}-${pkgver}.tar.gz::https://opensources.eckner.net/dl.php?dl=${_srcpkgname}-${pkgver}"
)
sha512sums=('d594bc689feb5514f624ca4b005b7e9a973b5024c0342eb84a0ccc97778bb912211ba554da7f85a8e54d3142b8165eb50b37ce8be5961eae121c1abed3827bfe')

package() {

    ls -la

    cd ${_srcpkgname}-${pkgver}
    make DESTDIR=$pkgdir install

