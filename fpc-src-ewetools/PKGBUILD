license=('GPL')
groups=()
depends=()
makedepends=()
checkdepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
options=()
source=(
    "${_srcpkgname}-${pkgver}.tar.gz::https://opensources.eckner.net/dl.php?dl=${_srcpkgname}-${pkgver}"
)
sha512sums=('598204f6baebeea31b7b2fef7b2fa71a6b2294d173eeff80fbbcb88fa01b7284f7d6465d73d715099472776eda6e90ed6ccbbdb7a5e92c57d069fa15baf1bd3f')

package() {

    ls -la

    cd ${_srcpkgname}-${pkgver}
